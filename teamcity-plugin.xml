<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>dotMemoryUnit</name>
    <display-name>JetBrains dotMemory Unit</display-name>
    <version>@Version@</version>
    <description>Provides the ability to run a build step under JetBrains dotMemory Unit for the .Net related build runners in  TeamCity.</description>
    <download-url>https://github.com/JetBrains/teamcity-dotmemory</download-url>
    <email>Nikolay.Pianikov@jetbrains.com</email>
    <vendor>
      <name>JetBrains</name>
      <url>https://www.jetbrains.com/</url>
      <logo>https://www.jetbrains.com/company/docs/logo_jetbrains.png</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
